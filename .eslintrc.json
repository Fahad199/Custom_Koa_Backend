{
  "env": {
    "commonjs": true,
    "node": true,
    "es6": true,
    "mocha": true
  },
  "parserOptions": {
    "ecmaVersion": 2019
  },
  "extends": ["airbnb-base", "prettier"],
  "plugins": ["eslint-plugin-prettier"],
